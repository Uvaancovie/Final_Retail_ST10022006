@model Final_Retail.Models.FileUpload

<h2>Edit File Details</h2>

<form asp-action="Edit" method="post">
    <input type="hidden" asp-for="RowKey" />
    <div class="form-group">
        <label asp-for="FileName" class="control-label"></label>
        <input asp-for="FileName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="ContentType" class="control-label"></label>
        <input asp-for="ContentType" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="FileUrl" class="control-label"></label>
        <input asp-for="FileUrl" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-primary" />
    </div>
</form>
